<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<Pane fx:id="root" prefHeight="140.0" prefWidth="140.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="miniStore.store.ProductHolderController">
   <children>
      <ImageView fx:id="image" fitHeight="140.0" fitWidth="147.0" layoutX="-1.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true" />
      <VBox alignment="CENTER" layoutX="11.0" layoutY="66.0" prefHeight="61.0" prefWidth="119.0" spacing="5.0">
         <children>
            <Text fx:id="name" strokeType="OUTSIDE" strokeWidth="0.0" />
            <Text fx:id="price" strokeType="OUTSIDE" strokeWidth="0.0" />
         </children>
      </VBox>
   </children>
</Pane>
