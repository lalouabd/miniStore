<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Text?>

<HBox prefHeight="500.0" prefWidth="695.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="miniStore.store.DashboardController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="500.0" prefWidth="232.0" spacing="10.0">
         <children>
            <Circle fx:id="profimage" fill="DODGERBLUE" radius="56.0" stroke="BLACK" strokeType="INSIDE" />
            <Text fx:id="name" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
            <Text fx:id="email" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
            <Text fx:id="balance" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
            <Button fx:id="myproducts" mnemonicParsing="false" text="my products" />
            <Button fx:id="withdraw" mnemonicParsing="false" prefHeight="26.0" prefWidth="103.0" text="withdraw" />
            <HBox alignment="CENTER" prefHeight="26.0" prefWidth="197.0" spacing="5.0">
               <children>
                  <TextField fx:id="money" prefHeight="26.0" prefWidth="137.0" />
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="\$" />
               </children>
               <padding>
                  <Insets left="15.0" />
               </padding>
            </HBox>
            <Button fx:id="addmoney" mnemonicParsing="false" text="add money" />
            <Button fx:id="buy" mnemonicParsing="false" prefHeight="26.0" prefWidth="90.0" text="purchase" />
            <Button fx:id="sell" mnemonicParsing="false" prefHeight="26.0" prefWidth="93.0" text="sell" />
            <Button fx:id="logout" mnemonicParsing="false" prefHeight="26.0" prefWidth="90.0" text="log out" />
            <Text fx:id="errText" strokeType="OUTSIDE" strokeWidth="0.0" />
         </children>
         <padding>
            <Insets left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <ScrollPane prefHeight="500.0" prefWidth="474.0">
         <content>
            <VBox fx:id="infbox" alignment="CENTER" prefHeight="521.0" prefWidth="458.0" />
         </content>
      </ScrollPane>
   </children>
</HBox>
